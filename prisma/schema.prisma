generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Member {
  id        Int      @id @default(autoincrement())
  prefix    String
  firstName String
  lastName  String
  birthDate DateTime
  gender    String?
  about     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
